<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GorselProgramlamaOdev2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GorselProgramlamaOdev2"
    xmlns:views="clr-namespace:GorselProgramlamaOdev2.MVVM.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
     Title="Kurlar"
     ContentTemplate="{DataTemplate views:DovizKuruView}"
     Route="DovizKuruView" />
    
    
    <ShellContent
   Title="Haberler"
   ContentTemplate="{DataTemplate views:HaberlerView}"
   Route="HaberlerView" />
    <ShellContent
Title="HavaDurumu"
ContentTemplate="{DataTemplate views:HavaDurumuView}"
Route="HavaDurumuView" />

    <ShellContent
Title="Yapilacaklar"
ContentTemplate="{DataTemplate views:Yapilacaklar}"
Route="Yapilacaklar" />
    <ShellContent
Title="Ayarlar"
ContentTemplate="{DataTemplate views:NewPage1}"
Route="NewPage1" />



</Shell>
